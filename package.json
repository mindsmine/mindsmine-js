{
  "name": "mindsmine-js",
  "version": "3.0.0",
  "main": "src/index.js",
  "description": "mindsmine-js",
  "authors": [
    "Mohammed Shaik Hussain Ali"
  ],
  "homepage": "http://mindsmine.shaiksphere.com/js",
  "bugs": "https://github.com/mindsmine/mindsmine-js/issues",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/mindsmine/mindsmine-js.git"
  },
  "engines": {
    "node": ">= 8.2.0"
  },
  "scripts": {
    "clean": "rimraf dist docs",
    "docs": "jsdoc --configure build/jsdoc.conf.json",
    "lint": "eslint scripts/**/* src/**/* test/**/*",
    "package": "npm run docs && npm run preclean",
    "preclean": "rimraf build",
    "predocs": "npm run test && node build/scripts/predocs",
    "pretest": "npm run uglify && node build/scripts/pretest",
    "preuglify": "npm run clean && npm run lint && babel scripts --out-dir build/scripts",
    "test": "jest final.test.js",
    "uglify": "node build/scripts/uglify"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-preset-es2017-node7": "0.5.2",
    "eslint": "4.4.1",
    "ink-docstrap": "1.3.0",
    "jest-cli": "20.0.4",
    "jsdoc": "3.5.3",
    "rimraf": "2.6.1",
    "uglify-es": "3.0.27"
  }
}
