{
  "name": "mindsmine-js",
  "version": "3.0.0",
  "main": "src/index.js",
  "description": "mindsmine-js",
  "authors": [
    "Mohammed Shaik Hussain Ali"
  ],
  "homepage": "http://mindsmine.shaiksphere.com/js",
  "bugs": "https://github.com/mindsmine/mindsmine-js/issues",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/mindsmine/mindsmine-js.git"
  },
  "engines": {
    "node": ">= 8.2.0"
  },
  "scripts": {
    "preclean": "del-cli build",
    "clean": "del-cli dist docs",
    "prebuild": "babel scripts --out-dir build/scripts",
    "build": "node build/scripts/build.js",
    "pretranspile": "npm run build",
    "transpile": "babel build/source/concatenated/index.js --out-file build/source/transpiled/index.js",
    "wip": "npm run clean && npm run transpile",
    "dev-build": "gulp uglify",
    "docs": "npm run test && gulp documentation",
    "lint": "eslint src/**/* test/**/*",
    "package": "npm run test && npm run preclean",
    "pretest": "npm run uglify",
    "test": "jest final.test.js",
    "uglify": "npm run clean && npm run lint && npm run transpile && gulp uglify"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "del-cli": "1.1.0",
    "eslint": "4.4.1",
    "jest-cli": "20.0.4",
    "jsdoc": "3.5.3",
    "gulp": "3.9.1",
    "gulp-jsdoc3": "1.0.1",
    "gulp-rename": "1.2.2",
    "gulp-uglify": "3.0.0"
  }
}
