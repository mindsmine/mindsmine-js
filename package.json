{
  "name": "mindsmine-js",
  "version": "3.2.0",
  "main": "src/index.js",
  "description": "mindsmine-js",
  "authors": [
    "Mohammed Shaik Hussain Ali"
  ],
  "homepage": "http://mindsmine.shaiksphere.com/js",
  "bugs": "https://github.com/mindsmine/mindsmine-js/issues",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/mindsmine/mindsmine-js.git"
  },
  "engines": {
    "node": ">= 10.3.0"
  },
  "scripts": {
    "clean": "rimraf dist docs",
    "docs": "jsdoc --configure build/jsdoc.conf.json",
    "lint": "eslint scripts/**/* src/**/* test/**/*",
    "package": "npm run docs && npm run preclean",
    "postlint": "babel scripts --out-dir build/scripts",
    "preclean": "rimraf build coverage",
    "predocs": "npm run test && node build/scripts/pre/docs",
    "pretest": "npm run uglify",
    "preuglify": "npm run clean && npm run lint && node build/scripts/pre/uglify",
    "test": "jest --coverage final.test.js",
    "uglify": "node build/scripts/uglify"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-env": "1.7.0",
    "eslint": "4.19.1",
    "ink-docstrap": "1.3.2",
    "jest-cli": "23.1.0",
    "jsdoc": "3.5.5",
    "rimraf": "2.6.2",
    "uglify-es": "3.3.9"
  }
}
