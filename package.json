{
  "name": "mindsmine-js",
  "version": "3.0.0",
  "main": "src/index.js",
  "description": "mindsmine-js",
  "authors": [
    "Mohammed Shaik Hussain Ali"
  ],
  "homepage": "http://mindsmine.shaiksphere.com/js",
  "bugs": "https://github.com/mindsmine/mindsmine-js/issues",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/mindsmine/mindsmine-js.git"
  },
  "engines": {
    "node": ">= 8.2.0"
  },
  "scripts": {
    "preclean": "del-cli build",
    "clean": "del-cli dist docs",

    "lint": "eslint scripts/**/* src/**/* test/**/*",

    "prebuild": "babel scripts --out-dir build/scripts",
    "build": "node build/scripts/build",

    "wip": "npm run clean && npm run build",
    "dev-build": "gulp uglify",

    "predocs": "npm run test",
    "docs": "jsdoc --configure build/scripts/helper/jsdoc.conf.json -t ./node_modules/ink-docstrap/template",

    "package": "npm run test && npm run preclean",

    "pretest": "npm run uglify",
    "test": "jest final.test.js",

    "preuglify": "npm run clean && npm run lint && npm run build",
    "uglify": "node build/scripts/uglify"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-preset-es2017-node7": "0.5.2",
    "del-cli": "1.1.0",
    "eslint": "4.4.1",
    "ink-docstrap": "1.3.0",
    "jest-cli": "20.0.4",
    "jsdoc": "3.5.3",
    "uglify-es": "3.0.27"
  }
}
