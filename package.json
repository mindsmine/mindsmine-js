{
  "name": "mindsmine-js",
  "version": "3.5.5",
  "main": "src/index.js",
  "description": "mindsmine-js",
  "authors": [
    "Mohammed Shaik Hussain Ali"
  ],
  "homepage": "http://mindsmine.shaiksphere.com/js",
  "bugs": "https://github.com/mindsmine/mindsmine-js/issues",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/mindsmine/mindsmine-js.git"
  },
  "engines": {
    "node": ">= 11.13.0"
  },
  "scripts": {
    "preclean": "node scripts/clean build coverage",
    "clean": "node scripts/clean dist docs",

    "lint": "eslint scripts/**/* src/**/* test/**/*",

    "preuglify": "npm run clean && npm run lint && babel scripts --out-dir build/scripts && node build/scripts/pre/uglify",
    "uglify": "node build/scripts/uglify",

    "pretest": "npm run uglify",
    "test": "jest --coverage final.test.js",

    "predocs": "npm run test && node build/scripts/pre/docs",
    "docs": "jsdoc --configure build/jsdoc.conf.json",

    "package": "npm run docs && npm run preclean"
  },
  "devDependencies": {
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "@babel/preset-env": "7.4.3",
    "eslint": "5.16.0",
    "ink-docstrap": "1.3.2",
    "jest-cli": "24.7.1",
    "jsdoc": "3.5.5",
    "uglify-es": "3.3.9"
  }
}
